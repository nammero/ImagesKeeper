{% extends 'base.html.twig' %}

{% block body %}
    <h1>Add new image</h1>
    {{ form_start(form) }}
    <div class="form-group col-md-12">
        {{ form_label(form.file) }}
        {{ form_widget(form.file) }}
    </div>
    <div class="form-group">
        <div class="col-md-12">
            {{ form_widget(form.submit, {attr: {class: "btn btn-primary"}}) }}
        </div>
    </div>
    {{ form_end(form) }}
{% endblock %}

